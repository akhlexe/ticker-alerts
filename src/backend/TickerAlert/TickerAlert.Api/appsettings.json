{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": {
    "AngularApp": "{AngularApp_BaseUrl}"
  },
  "ConnectionStrings": {
    "TickerAlertsDatabase": "{TickerAlertsDatabasePath}"
  },
  "Services": {
    "Finnhub": {
      "BaseUrl": "{Finnhub_BaseUrl}",
      "ApiKey": "{Finnhub_Api_Key}"
    }
  },
  "Jwt": {
    "Issuer": "{Jwt_Issuer}",
    "Audience": "{Jwt_Audience}",
    "Key": "{Jwt_Key}"
  }
}
